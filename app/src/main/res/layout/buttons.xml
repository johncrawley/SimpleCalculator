<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginBottom="@dimen/button_table_margin_bottom_portrait"
    tools:ignore="ButtonStyle">


            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonClear"
                style="?attr/editButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonBackspace"
                app:layout_constraintBottom_toTopOf="@id/buttonSine"
                android:text="@string/symbol_cancel" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonBackspace"
                style="?attr/editButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/buttonClear"
                app:layout_constraintEnd_toStartOf="@id/buttonMemorySet"
                app:layout_constraintBottom_toTopOf="@id/buttonCosine"
                android:text="@string/symbol_backspace" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonMemorySet"
                style="?attr/memoryButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="buttonMemorySet"
                app:layout_constraintEnd_toStartOf="@id/buttonMemoryRecall"
                app:layout_constraintBottom_toTopOf="@id/buttonTan"
                android:text="@string/symbol_memory_set" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonMemoryRecall"
                style="?attr/memoryButtonStyle"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/buttonMemorySet"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonPi"
                android:text="@string/symbol_memory_recall" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonSine"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonClear"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonCosine"
                app:layout_constraintBottom_toTopOf="@id/buttonPow"
                android:text="@string/symbol_sin" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonCosine"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonBackspace"
                app:layout_constraintStart_toEndOf="@id/buttonSine"
                app:layout_constraintEnd_toStartOf="@id/buttonTan"
                app:layout_constraintBottom_toTopOf="@id/buttonRoot"
                android:text="@string/symbol_cos" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonTan"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonMemorySet"
                app:layout_constraintStart_toEndOf="@id/buttonCosine"
                app:layout_constraintEnd_toStartOf="@id/buttonPi"
                app:layout_constraintBottom_toTopOf="@id/buttonPercent"
                android:text="@string/symbol_tan" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonPi"
                style="?attr/operatorButtonStyle"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonMemoryRecall"
                app:layout_constraintStart_toEndOf="@id/buttonTan"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonChangeSign"
                android:text="@string/symbol_pi" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonPow"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonSine"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonRoot"
                app:layout_constraintBottom_toTopOf="@id/button7"
                android:text="@string/symbol_power" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonRoot"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonCosine"
                app:layout_constraintStart_toEndOf="@id/buttonPow"
                app:layout_constraintEnd_toStartOf="@id/buttonPercent"
                app:layout_constraintBottom_toTopOf="@id/button8"
                android:text="@string/symbol_root" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonPercent"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonTan"
                app:layout_constraintStart_toEndOf="@id/buttonRoot"
                app:layout_constraintEnd_toStartOf="@id/buttonChangeSign"
                app:layout_constraintBottom_toTopOf="@id/button9"
                android:text="@string/symbol_percentage" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonChangeSign"
                style="?attr/operatorButtonStyle"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonPi"
                app:layout_constraintStart_toEndOf="@id/buttonPercent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonMultiply"
                android:text="@string/symbol_change_sign" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonPow"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/button8"
                app:layout_constraintBottom_toTopOf="@id/button4"
                android:text="@string/symbol_7"/>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonRoot"
                app:layout_constraintStart_toEndOf="@id/button7"
                app:layout_constraintEnd_toStartOf="@id/button9"
                app:layout_constraintBottom_toTopOf="@id/button5"
                android:text="@string/symbol_8"/>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonPercent"
                app:layout_constraintStart_toEndOf="@id/button8"
                app:layout_constraintEnd_toStartOf="@id/buttonMultiply"
                app:layout_constraintBottom_toTopOf="@id/button6"
                android:text="@string/symbol_9" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonMultiply"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonChangeSign"
                app:layout_constraintStart_toEndOf="@id/button9"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonMinus"
                android:text="@string/symbol_multiply" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button7"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/button5"
                app:layout_constraintBottom_toTopOf="@id/button1"
                android:text="@string/symbol_4"/>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button8"
                app:layout_constraintStart_toEndOf="@id/button4"
                app:layout_constraintEnd_toStartOf="@id/button6"
                app:layout_constraintBottom_toTopOf="@id/button2"
                android:text="@string/symbol_5" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button9"
                app:layout_constraintStart_toEndOf="@id/button5"
                app:layout_constraintEnd_toStartOf="@id/buttonMinus"
                app:layout_constraintBottom_toTopOf="@id/button3"
                android:text="@string/symbol_6"/>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonMinus"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonMultiply"
                app:layout_constraintStart_toEndOf="@id/button6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonPlus"
                android:text="@string/symbol_subtract" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/button2"
                app:layout_constraintBottom_toTopOf="@id/button0"
                android:text="@string/symbol_1"  />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button5"
                app:layout_constraintStart_toEndOf="@id/button1"
                app:layout_constraintEnd_toStartOf="@id/button3"
                app:layout_constraintBottom_toTopOf="@id/buttonDecimal"
                android:text="@string/symbol_2" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/button3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/button6"
                app:layout_constraintStart_toEndOf="@id/button2"
                app:layout_constraintEnd_toStartOf="@id/buttonPlus"
                app:layout_constraintBottom_toTopOf="@id/buttonEquals"
                android:text="@string/symbol_3"/>

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonPlus"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toBottomOf="@id/buttonMinus"
                app:layout_constraintStart_toEndOf="@id/button3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toTopOf="@id/buttonDivide"
                android:text="@string/symbol_add"/>


            <com.google.android.material.button.MaterialButton
                android:id="@+id/button0"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomtOf="parent"
                android:text="@string/symbol_0" />


            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonDecimal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomtOf="parent"
                android:text="@string/symbol_decimal" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonEquals"
                style="?attr/equalsButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomtOf="parent"
                android:text="@string/symbol_equals" />

            <com.google.android.material.button.MaterialButton
                android:id="@+id/buttonDivide"
                style="?attr/operatorButtonStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/button_margin"
                android:minHeight="@dimen/button_minHeight"
                android:minWidth="@dimen/button_minWidth"
                android:paddingHorizontal="@dimen/button_paddingHorizontal"
                android:paddingVertical="@dimen/button_paddingVertical"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomtOf="parent"
                android:text="@string/symbol_divide" />

</androidx.constraintlayout.widget.ConstraintLayout>